      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
    </module>
  </portlet>
    <portlet>
        <name>clientPortlet</name>
        <module>
            <script>
                <minify>false</minify>
                <path>/js/client.bundle.js</path>
